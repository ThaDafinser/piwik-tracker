{
    "name": "thadafinser/piwik-tracker",
    "description": "piwik php tracker file only",
    "keywords": ["piwik","web","analytics"],
    "homepage": "http://piwik.org",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "The Piwik Team",
            "email": "hello@piwik.org",
            "homepage": "http://piwik.org/the-piwik-team/"
        }
    ],
    "support": {
        "forum": "http://forum.piwik.org/",
        "issues": "http://dev.piwik.org/trac/roadmap",
        "wiki": "http://dev.piwik.org/",
        "source": "https://github.com/piwik/piwik"
    },
    "require": {
        "php":                     ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit":             "~4.0"
    },
    
    "autoload": {
        "classmap": [ "PiwikTracker.php"],
        "psr-0": {
            "PiwikPhpTracker\\": "src"
        }
    },
    
     "autoload-dev": {
        "psr-0": {
            "PiwikPhpTrackerTest\\": "tests"
        }
    }
}
